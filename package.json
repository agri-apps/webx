{
  "name": "webx1",
  "version": "1.0.3",
  "description": "",
  "source": "src/index.js",
  "main": "dist/webx1.js",
  "module": "dist/webx1.module.js",
  "unpkg": "dist/webx1.umd.js",
  "files": [
    "dist",
    "dist/plugins"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:main && npm run build:plugins",
    "build:main": "microbundle",
    "build:plugins": "./scripts/build-plugins.sh",
    "clean": "rm -rf dist",
    "dev": "microbundle watch",
    "copy:assets": "cp dist/webx1*.umd* demo/ && cp dist/plugins/* demo/plugins",
    "serve": "live-server ./demo --port=8080 --entry-file=index.html",
    "demo": "npm run build && npm run copy:assets && npm run serve",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },
  "keywords": [],
  "author": "john robinson <johnrob1880@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.12.3",
    "parcel-plugin-sw-cache": "^0.3.1"
  }
}
