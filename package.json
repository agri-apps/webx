{
  "name": "webx1",
  "version": "1.0.15",
  "description": "Simple and fast web applications",
  "source": "src/index.js",
  "main": "dist/webx1.js",
  "module": "dist/webx1.module.js",
  "unpkg": "dist/webx1.umd.js",
  "files": [
    "dist",
    "dist/plugins"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:main",
    "build:main": "microbundle",
    "clean": "rm -rf dist",
    "dev": "microbundle watch",
    "copy:assets": "cp dist/webx1*.umd* demo/",
    "serve": "live-server ./demo --port=8080 --entry-file=index.html",
    "demo": "npm run build && npm run copy:assets && npm run serve",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "web"
  ],
  "author": "john robinson <johnrob1880@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "microbundle": "^0.12.3"
  },
  "dependencies": {}
}
